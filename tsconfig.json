{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "artifacts/*": ["./artifacts/*"],
      "@charmverse/core/protocol": ["./node_modules/@charmverse/core/dist/cjs/protocol"],
      "@charmverse/core/log": ["./node_modules/@charmverse/core/dist/cjs/lib/log"]
    },
    "types": ["jest"]
  }
}
